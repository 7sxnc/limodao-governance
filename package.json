{
  "name": "Swipealot",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "rm -rf .parcel-cache dist node_modules yarn.lock",
    "dev": "NETWORK=kusama parcel serve ./src/index.html",
    "build": "NETWORK=kusama parcel build ./src/index.html --no-scope-hoist --log-level verbose --public-url https://jeluard.github.io/swipealot/"
  },
  "app": "dist/index.html",
  "targets": {
    "app": {}
  },
  "source": "src/index.tsx",
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "@nextui-org/react": "1.0.0-beta.10",
    "@parcel/service-worker": "2.8.0",
    "@polkadot-onboard/core": "^0.0.2-beta",
    "@polkadot-onboard/injected-wallets": "^0.0.2-beta",
    "@polkadot-onboard/react": "^0.0.2-beta",
    "@polkadot/api": "9.9.1",
    "@react-spring/web": "9.5.5",
    "framer-motion": "7.6.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-iconly": "2.2.5",
    "react-remark": "2.1.0"
  },
  "devDependencies": {
    "@parcel/compressor-brotli": "2.8.0",
    "@parcel/compressor-gzip": "2.8.0",
    "@parcel/packager-raw-url": "2.8.0",
    "@parcel/transformer-webmanifest": "2.8.0",
    "@types/react-dom": "18.0.6",
    "buffer": "5.5.0",
    "parcel": "2.8.0",
    "process": "0.11.10"
  },
  "alias": {
    "@polkadot/x-fetch": "./node_modules/@polkadot/x-fetch/browser.js",
    "@polkadot/x-ws": "./node_modules/@polkadot/x-ws/browser.js",
    "@polkadot/x-textencoder": "./node_modules/@polkadot/x-textencoder/browser.js",
    "@polkadot/x-textdecoder": "./node_modules/@polkadot/x-textdecoder/browser.js"
  }
}
